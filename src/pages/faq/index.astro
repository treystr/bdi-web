---
import Layout from "@layouts/Layout.astro"
import Faqs from '@components/sections/Faqs.astro';
import { faqLists } from '@data/faqs';

const seoTitle = "Frequently Asked Questions";
const seoDescription = "Find answers to common questions about the Bitcoin District Initiative, our programs, and Bitcoin education resources.";

const faqsSection = {
    title: 'Frequently Asked Questions',
    description: 'Find answers to common questions about the Bitcoin District Initiative, our programs, and Bitcoin education resources.',
    faqs: faqLists.main.faqs
}
---

<Layout title={seoTitle} description={seoDescription}>
    <Faqs content={faqsSection} background="base" paddingTop="large" paddingBottom="large" />
</Layout>